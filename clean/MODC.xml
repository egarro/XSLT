<form formPrefix="MODC" charityName="004" presentationStyle="grouped">
    
    <requireditemcolor>#FFCDAD</requireditemcolor>
    <optionalitemcolor>#A6FEFF</optionalitemcolor>
    <leftFormMargin>20.0</leftFormMargin>
    <rightFormMargin>280.0</rightFormMargin>
    <pageWidth>680.0</pageWidth>
    
    
    
    <page name="firstPage">
        <bgcolor>#eeeeee</bgcolor>
        <leftPageMargin>100.0</leftPageMargin>
        <rightPageMargin>100.0</rightPageMargin>  
        <group name="header">
            <item name="logo" type="zipImage">
                <path>MODC_Logo.png</path>
                <position>upper_right</position>
                <width>280</width>
                <height>80</height> 
            </item> 
            <item name="_internalFormId" type="textField" editable="NO" isHidden="YES">
                <size>250</size>
                <label>Reference Number</label>
                <position>upper_left</position>
                <placeholder>CDC25</placeholder>
            </item>
            <item name="repId" type="textField" editable="NO" isHidden="YES">
                <size>620</size>
                <textSize>18</textSize> 
                <position>center</position>
                <placeholder>Fundraiser ID</placeholder>
            </item>
        </group>
    </page>
    
    <page name="secondPage">
        <bgcolor>#cccccc</bgcolor>
        <leftPageMargin>20.0</leftPageMargin>
        <rightPageMargin>20.0</rightPageMargin> 
