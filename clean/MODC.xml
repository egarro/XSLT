            <item name="donationType"
               type="radioControl"
               multiple="NO"
               required="YES">
                <size>620</size>
                <columns>2</columns>
                <tag value="individual">Individual</tag>
                <tag value="business">Business</tag>
                <label>This donation is made on behalf of:</label>
            </item>
            <item name="line1" type="lineBlock">
                <size>5</size>
            </item>    
            <item name="gender" type="radioControl" multiple="NO" required="YES">
                <size>620</size>
                <columns>2</columns>
                <tag value="Male">Male</tag>
                <tag value="Female">Female</tag>       
                <label>Gender</label>
                <onmissing>
               <message>Please, select a gender.</message>
            </onmissing>
            </item>
            <item name="line2" type="lineBlock">
                <size>5</size>
            </item>
            <item name="titleMale"
               type="radioControl"
               multiple="NO"
               required="YES">
                <size>620</size>
                <columns>3</columns>
                <tag value="MR">Mr</tag>
                <tag value="DR">Dr</tag> 
                <tag value="OTHER">Other</tag>
                <label>Title</label>
                <submitName>title</submitName>
                <onmissing>
               <message>Please, select a title.</message>
            </onmissing>
            </item>
            <item name="titleFemale"
               type="radioControl"
               isHidden="YES"
               multiple="NO"
               required="YES">
                <size>620</size>
                <columns>3</columns>
                <tag value="MRS">Mrs</tag>
                <tag value="MS">Ms</tag>
                <tag value="MISS">Miss</tag>
                <tag value="DR">Dr</tag> 
                <tag value="OTHER">Other</tag>
                <label>Title</label>
                <submitName>title</submitName>
                <onmissing>
               <message>Please, select a title.</message>
            </onmissing>
            </item>
            <item name="otherTitleM"
               type="textField"
               isHidden="YES"
               required="YES">
                <size>150</size>
                <maxlength>4</maxlength>
                <submitName>title</submitName>
                <placeholder>Other</placeholder>
                <validation>lettersOnly</validation>
                <onmissing>
               <message>Please, select a title.</message>
            </onmissing>
            </item>
            <item name="otherTitleF"
               type="textField"
               isHidden="YES"
               required="YES">
                <size>150</size>
                <maxlength>4</maxlength>
                <submitName>title</submitName>
                <placeholder>Other</placeholder>
                <validation>lettersOnly</validation>
                <onmissing>
               <message>Please, select a title.</message>
            </onmissing>
            </item>  
            <item name="line3" type="lineBlock">
                <size>5</size>
            </item>
            <item name="firstName" type="textField" required="YES">
                <size>620</size>
            <textSize>26</textSize>
                <label>First Name</label>
                <placeholder>First Name</placeholder>
                <validation>names</validation>
                <maxlength>20</maxlength>
                <onmissing>
               <message>Please, enter your name.</message>
            </onmissing>    
            </item>
            <item name="lastName" type="textField" required="YES">
                <size>620</size>
            <textSize>26</textSize>
                <label>Last Name</label>
                <placeholder>Last Name</placeholder>
                <validation>names</validation>
                <maxlength>20</maxlength>
            </item>
            <item name="businessName"
               type="textField"
               isHidden="YES"
               required="YES">
                <size>620</size>
            <textSize>26</textSize>
                <label>Business Name</label>
                <placeholder>Business Name</placeholder>
                <validation>names</validation>
            </item>
            <item name="birthDate" type="dateField" required="YES">
                <label>Date of birth</label>
                <formatting>DD/MM/YYYY</formatting>
                <size>620</size>
            <textSize>26</textSize>
                <prefix>birth</prefix>
                <minAgeRestriction>21</minAgeRestriction>
                <maxAgeRestriction>76</maxAgeRestriction>
                <onmissing>
               <message>Please, enter your date of birth.</message>
            </onmissing>
            </item> 
            <item name="address1" type="addressField" required="YES">
                <size>630</size>
            <textSize>26</textSize>
                <country>CANADA</country>
                <validation>QAS</validation>
            </item> 
            <item name="phone1" type="textField" required="phone1|phone2">
                <size>620</size>
            <textSize>26</textSize>
                <placeholder>Home Phone</placeholder>
                <validation>usphonenumber</validation>
            </item>
            <item name="phone2" type="textField" required="phone1|phone2">
                <size>620</size>
            <textSize>26</textSize>
                <placeholder>Cell Phone</placeholder>
                <validation>usphonenumber</validation>
            </item>
            <item name="phone3" type="textField" isHidden="YES">
                <size>620</size>
            <textSize>26</textSize>
                <placeholder>Business Phone</placeholder>
                <validation>usphonenumber</validation>
            </item>
            <item name="phoneExtension" type="textField" isHidden="YES">
                <size>620</size>
            <textSize>26</textSize>
                <placeholder>Ext</placeholder>
                <validation>numericOnly</validation>
                <maxlength>7</maxlength>
            </item>
            <item name="emailRequired"
               type="emailPinger"
               submitAlerts="YES"
               mustValidate="NO">
                <size>620</size>
                <textSize>26</textSize>
                <submitName>email</submitName>
            </item>
            <item name="emailLabel" type="label" bleeds="YES">
                <text>At the end of this year, your tax receipt will be emailed to this address.</text>
                <textColor>#111111</textColor>
                <textSize>12</textSize>
            </item>
            <item name="line4" type="lineBlock">
                <size>5</size>
            </item>
        </group>
    </page>
    
    <page name="thirdPage">
        <bgcolor>#aaaaaa</bgcolor>
        <group name="secondSection" number="2" title="Regular Donation">
            <item name="group2Label" type="label" bleeds="YES">
                <text>I wish to make an automatic monthly contribution to:</text>
                <textColor>#111111</textColor>
                <textSize>14</textSize>
            </item> 
            <item name="addressBanner" type="zipImage" bleeds="YES">
                <path>address_zip.png</path>
                <width>650</width>
                <height>187</height>
            </item>
            <item name="frequency"
               type="radioControl"
               multiple="NO"
               isHidden="YES"
               forceSubmit="YES">
                <size>310</size>
                <columns>1</columns>
                <tag value="1">Monthly</tag>
                <defaultValue>1</defaultValue>
                <label>Recurring:</label>
            </item>
            <item name="_amount" type="valuePicker" required="YES">
                <size>620</size>
                <textSize>26</textSize>
                <placeholder>Donation amount</placeholder>
                <option value="10">10</option>
                <option value="15">15</option>
                <option value="20">20</option>
                <option value="25">25</option>
                <order>descending</order>
                <action>itemShouldNotify:</action>
            </item>
            <item name="firstGiftAmount"
               type="textField"
               required="YES"
               isHidden="YES"
               forceSubmit="YES">
                <size>620</size>
                <textSize>26</textSize>
            </item>
            <item name="collectionDatePrint"
               type="multilineLabel"
               required="NO"
               bleeds="YES">
                <size>550</size>
                <sizeY>125</sizeY>
                <position>center</position>
                <numberOfLines>2</numberOfLines>
                <textColor>#111111</textColor>
                <textSize>14</textSize>
                <bgcolor>clear</bgcolor>
                <text>Your first gift will be processed today and the following monthly donations will be withdrawn on the:</text>
            </item>
            <item name="collectionDate"
               type="radioControl"
               multiple="NO"
               required="YES">
                <size>620</size>
                <columns>2</columns>
                <tag value="15">15th of each month</tag>
                <tag value="30">30th of each month</tag>
            </item>
            <item name="line8" type="lineBlock">
                <size>5</size>
            </item>                        
            <item name="businessId" type="textField" editable="NO">
                <size>620</size>
            <textSize>26</textSize>
                <placeholder>879585214RR0001</placeholder>
                <label>Charitable Business Number:</label>
            </item>
            
            <item name="sign1" type="signatureBox" required="YES">
                <position>center</position>
                <width>200</width>
                <height>80</height> 
                <label>Donor Initials</label>
                <detailLabel>I understand that this is an ongoing monthly contribution and my first gift will be processed today.</detailLabel>
                <legalText>I understand that this is an ongoing monthly contribution and my first gift will be processed today.</legalText>
            </item>
            
            <item name="finePrint3"
               type="multilineLabel"
               required="NO"
               bleeds="YES">
                <size>550</size>
                <sizeY>125</sizeY>
                <position>center</position>
                <numberOfLines>5</numberOfLines>
                <textColor>#111111</textColor>
                <textSize>14</textSize>
                <bgcolor>clear</bgcolor>
                <text>I may revoke my authorization or make changes to my donation at any time, subject to providing notice of 10 days. To obtain a sample cancellation form, or for more information on my right to cancel a PAD agreement, I may contact my financial institution or visit www.cdnpay.ca</text>
            </item>
            <item name="line5" type="lineBlock">
                <size>5</size>
            </item>
        </group>
    </page>    
    
    <page name="fourthPage">
        <bgcolor>#888888</bgcolor>
        <group name="thirdSection" number="3" title="Method of Payment">
            <item name="methodOfCollection"
               type="radioControl"
               multiple="NO"
               required="YES">
                <size>620</size>
                <columns>2</columns>
                <tag value="3">Credit Card</tag>
                <tag value="1">Bank</tag>
                <label>Pay by:</label>
                <defaultValue>3</defaultValue>
            </item>
        </group>
        
        <group name="bankInfo" isHidden="YES">
            <item name="transitNumber" type="textField" required="YES">
                <size>620</size>
            <textSize>26</textSize>
                <placeholder>Transit Number</placeholder>
                <validation>numericOnly</validation>
                <maxlength>5</maxlength>
                <minlength>5</minlength>
            </item>
            <item name="bankNumber" type="textField" required="YES">
                <size>620</size>
            <textSize>26</textSize>
                <placeholder>Bank Number</placeholder>
                <validation>numericOnly</validation>
                <maxlength>3</maxlength>
                <minlength>3</minlength>
                <label>Bank Number</label>
            </item>
            <item name="accountNumber"
               type="textField"
               required="YES"
               password="YES">
                <size>620</size>
            <textSize>26</textSize>
                <placeholder>Account Number</placeholder>
                <validation>numericOnly</validation>
                <maxlength>12</maxlength>
                <minlength>6</minlength>
                <label>Account Number</label>
            </item>
            <item name="repeatAccountNumber"
               type="textField"
               required="YES"
               password="YES">
                <size>620</size>
            <textSize>26</textSize>
                <placeholder>Repeat Account Number</placeholder>
                <validation>numericOnly</validation>
                <maxlength>12</maxlength>
            </item>
