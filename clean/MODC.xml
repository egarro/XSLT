               type="signatureBox"
               bleeds="YES"
               required="YES">
                <position>center</position>
                <width>250</width>
                <height>80</height> 
                <submitName>sign2</submitName>
                <label>I am proud to be a monthly donor:</label>
                <detailLabel>Donor authorizing monthly pledge and payment details.</detailLabel>
                <legalText>Donor authorizing monthly pledge and payment details.</legalText>
            </item>
