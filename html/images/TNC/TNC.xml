                <size>620</size>
                <textSize>26.0</textSize>
                <label>Fundraiser State:</label>
            </item>
            <item name="fundraiserCity" type="dropDown" required="YES">
                <size>620</size>
                <textSize>26.0</textSize>
                <label>Fundraiser City:</label>
            </item>
            <item name="receivedWp" type="radioControl" multiple="NO" required="YES">
                <size>620</size>
                <columns>3</columns>
                <tag value="Y">Yes</tag>
                <tag value="N">No</tag>
                <tag value="D">Declined</tag>
                <label language="EN_US"> Did the donor receive a Charity Welcome Information Package?</label>
            </item>
         </group>
